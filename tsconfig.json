{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext"],
    "strict": false,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "protractor", "jasmine"],
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  },
  "include": ["lib", "framework", "specs", "data", "pages"],
  "exclude": ["node_modules"]
}
